{
  "name": "db",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/db",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["docker build -f ./apps/db/Dockerfile -t insta-db ."]
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "docker rm -f insta-db || true",
          "docker run -d --name insta-db -p 27018:27017 --network insta-cloud -v ./apps/db/data:/data/db -v ./apps/db/config:/config --restart always insta-db"
        ],
        "parallel": false
      }
    }
  },
  "tags": []
}
