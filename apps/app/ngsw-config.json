{
  "$schema": "../../node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/",
          "/favicon.ico",
          "/index.html",
          "/manifest.json",
          "/ngsw.json",
          "/*.css",
          "/*.js",
          "/assets/icons/*"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "navigationUrls": ["/**", "!/**/*.{js,css,json,ico}"]
}
